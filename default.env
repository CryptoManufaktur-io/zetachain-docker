COMPOSE_FILE=zetachain.yml
MONIKER=zetachain-docker
# Choose network. Options are mainnet or testnet.
NETWORK=mainnet

EXTRA_FLAGS=
COSMOVISOR_VERSION=v1.5.0
ZETACORED_VERSION=v22.0.2

# Use latest from https://polkachu.com/tendermint_snapshots/zetachain
SNAPSHOT_URL=

P2P_PORT=26656
RPC_PORT=26657

# Secure web proxy - advanced use, please see instructions
DOMAIN=example.com
RPC_HOST=zetachain
RPC_LB=zetachain-lb

# info, warn, error, trace
LOG_LEVEL=info

# You can pin the version of allora-docker here
SCRIPT_TAG=

# Used by allorad update - please do not adjust
ENV_VERSION=1
